{
  "name": "atompulse/craft",
  "description": "Atompulse Craft",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Petru Cojocar",
      "email": "petru.cojocar@gmail.com"
    }
  ],
  "repositories": [
  ],
  "autoload": {
    "psr-4": {
      "Craft\\": "src/Craft/",
      "CraftBundle\\": "src/CraftBundle/"
    },
    "exclude-from-classmap": [
      "**/Tests/"
    ]
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-intl": "*",
    "doctrine/annotations": "1.8.*",
    "doctrine/inflector": "^1.3",
    "paragonie/paseto": "^1.0",
    "symfony/expression-language": "4.4.*",
    "symfony/routing": "4.4.*",
    "symfony/security-bundle": "4.4.*",
    "symfony/validator": "4.4.*",
    "symfony/yaml": "4.4.*"
  },
  "require-dev": {
  },
  "suggest": {
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "extra": {},
  "minimum-stability": "dev",
  "prefer-stable": true
}
