{
  "name": "atompulse/craft",
  "description": "Atompulse Craft",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Petru Cojocar",
      "email": "petru.cojocar@gmail.com"
    }
  ],
  "repositories": [
  ],
  "autoload": {
    "psr-4": {
      "Craft\\": "src/Craft/",
      "CraftBundle\\": "src/CraftBundle/"
    },
    "exclude-from-classmap": [
      "**/Tests/"
    ]
  },
  "require": {
    "php": "^7.2",
    "ext-intl": "*",
    "ext-json": "*",
    "paragonie/paseto": "^1.0",
    "symfony/config": "4.4.*",
    "symfony/dependency-injection": "4.4.*",
    "symfony/expression-language": "4.4.*",
    "symfony/http-foundation": "4.4.*",
    "symfony/routing": "4.4.*",
    "symfony/security": "^4.4",
    "symfony/validator": "4.4.*",
    "symfony/yaml": "4.4.*"
  },
  "require-dev": {
    "byteout/psr-logger-codeception-module": "1.1.*",
    "codeception/codeception": "4.1.*",
    "codeception/module-asserts": "^1.1"
  },
  "suggest": {
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "extra": {},
  "minimum-stability": "dev",
  "prefer-stable": true
}
